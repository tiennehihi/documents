:root {
    --height-header: 46px;
}

* {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

html {
    scroll-behavior: smooth;
    font-family: 'Lato', sans-serif;
}
.text-white{
    color: #fff;
}
/* Common */
.clear {
    clear: both;
}

.text-white {
    color: #fff !important;
}

.text-center {
    text-align: center !important;
}

.mt-8 {
    margin-top: 8px !important;
}

.mt-16 {
    margin-top: 16px !important;
}

.mt-32 {
    margin-top: 32px !important;
}

.mt-48 {
    margin-top: 48px;
}

.row {
    margin-left: -8px;
    margin-right: -8px;
}

.row::after {
    content: "";
    display: block;
    clear: both;
}

.col {
    float: left;
    padding-left: 8px;
    padding-right: 8px;
}

.col-third {
    width: 33.33333%;
}

.col-half {
    width: 50%;
}

.col-full {
    width: 100%;
}

.center {
    text-align: center;
}

/* Header */
#header {
    height: var(--height-header);
    background-color: #000;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 1;
}

#nav {
    display: inline-block;
}

#nav > li {
    display: inline-block;
}

#nav li {
    position: relative;
}

#nav li:hover .subnav {
    display: block;
}

#nav > li > a {
    color: white;
    text-decoration: none;
    text-transform: uppercase;
    line-height: var(--height-header);
    display: block;
}

#nav li a {
    padding: 0 24px;
}

#nav .subnav {
    display: none;
    position: absolute;
    top: 100%;
    left: 0;
    min-width: 160px;
    background-color: #fff;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
}

#nav .subnav a {
    color: #000;
    padding: 0 12px;
    line-height: 38px;
    text-decoration: none;
}

#nav, .subnav {
    list-style-type: none;
}

#nav .subnav li:hover,
#nav > li:hover > a {
    color: #000;
    background-color: #ccc;
    cursor: pointer;
}

#header .search-btn {
    float: right;
    padding: 0 21px;
}

#header .search-icon {
    color: #fff;
    font-size: 20px;
    line-height: var(--height-header);
}
#header .search-btn:hover {
    cursor: pointer;
    background-color: #f44336;
}

/* Slider */
#slider {
    position: relative;
    margin-top: var(--height-header);
    background-image: url('../images/slider/chicago.jpg') top center / cover no-repeat;
    padding-top: 50%;
    background-position: top center;
    background-repeat: no-repeat;
    background-size: cover;
    /* animation: fadeIn 3s infinite; */
}
/* 
.fade-in {
  opacity: 0;
  transition: opacity 0.5s ease-in-out;
}

.fade-in.active {
  opacity: 1;
} */

@keyframes fadeIn {
    from {
        opacity: 0.6;
    } to {
        opacity: 1;
    }
}

#slider .text-content {
    position: absolute;
    color: #fff;
    text-align: center;
    width: 100%;
    bottom: 70px;
}

#slider .text-heading {
    font-weight: 500;
    font-size: 24px;
}

#slider .text-description {
    font-size: 15px;
    margin-top: 25px;
}

/* Content Section */
#content .content-section {
    width: 800px;
    max-width: 100%;
    /* text-align: center; */
    margin-left: auto;
    margin-right: auto;
    padding: 64px 0 112px;
}

#content .section-heading {
    font-size: 30px;
    font-weight: 500;
    text-align: center;
    letter-spacing: 4px;
    text-transform: uppercase;
}

#content .section-sub-heading {
    font-size: 15px;
    text-align: center;
    margin-top: 25px;
    font-style: italic;
    opacity: 0.6;
}

#content .about-text {
    margin-top: 25px;
    line-height: 1.4;
    text-align: justify;
}

#content .member-list {
    margin-top: 32px;
}

#content .member-item {
    float: left;
    padding: 48px 0px 0px;
    width: calc(100% / 3);
    text-align: center;
}

#content .member-name {
    margin: 15px 0px;
    font-size: 15px;
}

#content .member-img {
    /* margin: 0px 0px 16px 0px; */
    margin-top: 15px;
    width: 154px;
    border-radius: 4px;
}

/* Tour section */
.tour-section {
    background: #000;
    /* padding-top: 80px; */
}

.ticket-list {
    margin-top: 40px;
    /* color: #fff; */
    list-style: none;
    background-color: #fff;
    /* width: 800px; */
}

.ticket-list li {
    font-size: 15px;
    padding: 12px 16px;
    border-bottom: 1px solid #ccc;
    color: #757575;
}

.ticket-list .sold-out {
    background-color: #f44336;
    padding: 3px 4px;
    color: #fff;
    margin-left: 16px;
}

.ticket-list .quantity {
    float: right;
    background-color: #000;
    color: #fff;
    margin-right: 24px;
    width: 24px;
    height: 24px;
    text-align: center;
    line-height: 24px;
    border-radius: 50%;
    margin-top: -3px;
}

/* Place */
/* .place-list {
    margin-top: 32px;
} */

/* .place-item {
    width: 33.333333%;
    padding: 0 8px;
    float: left;
} */

.place-img {
    width: 100%;
    display: block;
}

.place-img:hover {
    opacity: 0.6;
}

.place-body {
    background-color: #fff;
    padding: 15px 16px;
    font-size: 15px;
}

.place-heading {
    font-size: 15px;
    font-weight: 600;
}

.place-time {
    margin-top: 15px;
    color: #757575;
}

.place-desc {
    margin-top: 15px;
    line-height: 1.4;
    font-weight: 500;
}

.place-buy-btn {
    padding: 11px 16px;
    background-color: #000;
    display: inline-block;
    margin-top: 15px;
    text-decoration: none;
    color: #fff;
    text-align: center;
    border: none;
}

.place-buy-btn:hover {
    background-color: #ccc;
    color: #000;
}

/* Modal */
.modal {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.4);
    align-items: center;
    justify-content: center;
    display: none;
}

.modal.open {
    display: flex;
}

.modal-container {
    background: #fff;
    width: 900px;
    max-width: calc(100% - 32px);
    position: relative;
    height: auto;
    animation: modalFadeIn ease .5s;
}

.modal-close {
    position: absolute;
    top: 0;
    right: 0;
    color: #fff;
    padding: 16px;
    opacity: 0.9;
}

.modal-close:hover {
    background-color: #ccc;
    opacity: 1;
    cursor: pointer;
    color: #000;
}

.modal-header {
    background-color: #009688;
    display: flex;
    height: 130px;
    justify-content: center;
    align-items: center;
    font-size: 30px;
    color: #fff;
    letter-spacing: 4px;
}

.modal-heading-icon {
    margin: 16px;
}

.modal-body {
    padding: 16px;
}

.modal-label {
    display: block;
    font-size: 15px;
    margin-bottom: 15px;
}

.modal-input {
    border: 1px solid #ccc;
    width: 100%;
    padding: 10px;
    font-size: 15px;
    margin-bottom: 24px;
    cursor: text;
    border-radius: 4px;
}

#buy-ticket {
    color: #fff;
    background: #009688;
    width: 100%;
    margin: 16px 0;
    font-size: 15px;
    border: none;
    padding: 16px;
    text-transform: uppercase;
}

#buy-ticket:hover {
    background-color: #ccc;
    color: #000;
    cursor: pointer;
}

.modal-footer {
    text-align: right;
    padding: 16px;
}

.modal-footer a {
    color: #359ff4;
}

@keyframes modalFadeIn {
    from {
        opacity: 0;
        transform: translateY(-140px);
    }
    to {
        opacity: 1;
        transform: translateY(0px);
    }
}
/* Contact */
.contact-content {
    margin-top: 48px;
}

.contact-info {
    font-size: 18px;
    line-height: 1.5;
}

.contact-info i[class*="fa-"] {
    width: 30px;
    display: inline-block;
}

.contact-form {
    font-size: 15px;
}

.contact-form .form-control {
    padding: 10px;
    border: 1px solid #ccc;
    width: 100%;
    outline: none;
}

.contact-form .form-submit-btn {
    background-color: #000;
    border: none;
    font-size: 15px;
    color: #fff;
    padding: 10px 16px;
    float: right;
}

.contact-form .form-submit-btn:hover {
    background-color: #ccc;
    color: #000;
    cursor: pointer;
    border: none;
}

/* Map */
.map-section img {
    width: 100%;
    opacity: 0.9;
}

#footer {
    padding: 64px 16px;
    text-align: center;
}

#footer .socials-list a {
    font-size: 24px;
    color: rgba(0, 0, 0, 0.6);
}

#footer .socials-list a:hover {
    color: rgba(0, 0, 0, 0.4);
}

#footer .copyright {
    margin-top: 15px;
    color: rgba(0, 0, 0, 0.6);
}

#footer .copyright a {
    color: rgba(0, 0, 0, 0.6);
